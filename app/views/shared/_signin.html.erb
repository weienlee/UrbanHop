
<%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <div class="row-fluid" style="margin-top:11px">
		<div class="span3 offset5">
			<%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
		</div>
	  </div>
<% end %>
